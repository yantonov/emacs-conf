(require 'highlight-parentheses)

(defun yantonov/init-highlight-parentheses()
  (setq hl-paren-delay 0.2)
  (setq hl-paren-colors '("Springgreen3"
                          "IndianRed1"
                          "IndianRed3"
                          "IndianRed4")))

(provide 'emacs-rc-highlight-parentheses)
